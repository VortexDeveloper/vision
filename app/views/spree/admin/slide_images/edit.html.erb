<%= render partial: 'spree/admin/shared/slide_image_tabs', locals: {current: :details} %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @slide_image } %>

<%= form_for [:admin, @slide_image], method: :put, html: { multipart: true } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= button_link_to Spree.t('actions.voltar'), collection_url, :icon => 'delete' %>
  </fieldset>
<% end %>
