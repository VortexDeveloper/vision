<%= form_for @survey do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :cpf %>
  <%= f.text_field :cpf %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
<% end %>
